<?xml version="1.0"?>
<configuration>
  <appSettings>
    <!--屏幕刷新间隔，秒-->
    <add key="updateInterval" value="1"/>
    <!--写日志间隔，分钟-->
    <add key="logInterval" value="30"/>
    <!--日志文件名-->
    <add key="logFileName" value="log.log"/>
  </appSettings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
